{
  "name": "@monimmo/front",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.3.8",
  "scripts": {
    "ng": "./scripts/ng-node.sh",
    "start": "./scripts/ng-node.sh serve --port 5173 --host 127.0.0.1",
    "build": "./scripts/ng-node.sh build",
    "watch": "./scripts/ng-node.sh build --watch --configuration development",
    "test": "./scripts/ng-node.sh test --watch=false",
    "test:coverage": "./scripts/ng-node.sh test --watch=false --coverage && bun ../scripts/check-lcov-threshold.mjs coverage/lcov.info 80 front",
    "typecheck": "bunx ngc -p tsconfig.app.json && bunx tsc -p tsconfig.spec.json --noEmit"
  },
  "dependencies": {
    "@angular/common": "^21.2.0",
    "@angular/compiler": "^21.2.0",
    "@angular/core": "^21.2.0",
    "@angular/forms": "^21.2.0",
    "@angular/platform-browser": "^21.2.0",
    "@angular/router": "^21.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.2.0",
    "@angular/cli": "^21.2.0",
    "@angular/compiler-cli": "^21.2.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
